record(bo, "$(user):PS:MON")
{
    field(DESC, "Turn output off/on")
    field(DTYP, "stream")
    field(OUT,  "@pro.proto setMON DEV1")
    field(ZNAM, "Off")
    field(ONAM, "On")
}

