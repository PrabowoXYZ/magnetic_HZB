record (calcout, "$(user):PS:BOTTON")
{
    field(DESC, "ON/OFF")
    field(EGU, "BOOL")
    field(INPA, "$(user):PS:MON.VAL PP NMS")
    field(INPB, "$(user):PS:MOFF.VAL PP NMS")
    field(CALC, "A && (!B)")
}