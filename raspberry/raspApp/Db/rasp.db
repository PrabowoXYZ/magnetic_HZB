record(stringin, "$(user):IDN") {
	field(DTYP, "stream")
	field(INP, "@rasp.proto idn L0 PP")
}

record(ai, "$(user):FIELD") {
	field(DTYP, "stream")
	field(INP, "@rasp.proto readMag L0 PP")
}

record(ai, "$(user):VOLT") {
	field(DTYP, "stream")
	field(INP, "@rasp.proto readVolt L0 PP")
}

record(bi, "$(user):ONSTATUS") {
	field(DTYP, "stream")
	field(INP, "@rasp.proto readEnable L0 PP")
}

record(bo, "$(user):ONCTRL") {
	field(DTYP, "stream")
	field(OUT, "@rasp.proto setEnable L0 PP")
}
